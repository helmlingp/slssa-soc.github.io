{
  "responseOverrides": {
    "400": {
      "rewrite": "/400.html"
    },
    "401": {
      "redirect": "/.auth/login/auth0",
      "statusCode": 302
    },
    "403": {
      "rewrite": "/403.html"
    },
    "404": {
      "rewrite": "/404.html"
    }
  },
  "auth": {
      "identityProviders": {
          "customOpenIdConnectProviders": {
              "auth0": {
                  "registration": {
                      "clientIdSettingName": "AUTH0_ID",
                      "clientCredential": {
                          "clientSecretSettingName": "AUTH0_SECRET"
                      },
                      "openIdConnectConfiguration": {
                          "wellKnownOpenIdConfiguration": "https://https://dev-44fpvkcla3esxwq8.au.auth0.com/.well-known/openid-configuration"
                      }
                  },
                  "login": {
                      "nameClaimType": "http://schemas.xmlsoap.org/ws/2005/05/identity/claims/name",
                      "scopes": ["openid", "profile"]
                  }
              }
          }
      }
  }
}