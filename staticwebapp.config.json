{
  "routes": [
    {
      "route": "/*",
      "allowedRoles": [
        "authenticated"
      ]
    }
  ],
  "responseOverrides": {
    "400": {
      "rewrite": "/400.html"
    },
    "401": {
      "redirect": "/.auth/login/aad?post_login_redirect_uri=/",
      "statusCode": 302
    },
    "403": {
      "rewrite": "/403.html"
    },
    "404": {
      "rewrite": "/404.html"
    }
  },
  "auth": {
    "identityProviders": {
      "azureActiveDirectory": {
        "registration": {
          "openIdIssuer": "https://login.microsoftonline.com/0c31d2df-c736-4073-bf96-b344f9c3bca3/v2.0",
          "clientIdSettingName": "ee8f933c-49ee-4212-b5a3-609241384866",
          "clientSecretSettingName": "e235965a-b61c-4b0a-ab88-2441c80463bb"
        }
      }
    }
  }
}